  <%= f.text_field :title %>
  <br />

  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <br />

  <%= f.collection_select :genre_name, Genre.all, :name, :name %>

  <input name="song[note_contents][]">
  <input name="song[note_contents][]">
  <input name="song[note_contents][]">

  <%= f.submit %>
<% end %>
